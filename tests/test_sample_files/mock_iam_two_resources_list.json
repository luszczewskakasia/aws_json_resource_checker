{
    "PolicyName": "S3",
    "PolicyDocument": {
        "Version": "2012-10-17",
        "Statement": [
            {
                "Effect": "Allow",
                "Action": [
                    "s3:GetObject",
                    "s3:PutObject",
                    "s3:DeleteObject"
                ],
                "Resource": [
                    "arn:aws:s3:::my-app-logs/*"
                ]
            },
            {
                "Effect": "Allow",
                "Action": [
                    "foo"
                ],
                "Resource": [
                    "test-resource",
                    "test-resource2",
                    "test-resource3/*"
                ]
            }
        ]
    }
}
